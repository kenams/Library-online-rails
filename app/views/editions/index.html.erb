<h2>All Editions</h2>
<table class="table table-striped table-responsive">
  <thead>
    <th>Author</th>
    <th>Title</th>
    <th>Publisher</th>
    <th>Publication Year</th>
    <th>ISBN13</th>
  </thead>
<% @editions.each do |edition| %>
  <tr>
    <td><%= edition.authors[0].name %></td>
    <td><%= edition.work.title %></td>
    <td><%= edition.publisher.name %></td>
    <td><%= edition.publication_year %></td>
    <td><%= edition.isbn13 %></td>
  </tr>
<% end %>
</table
<p>
  <%= link_to 'New Edition', new_edition_path %>
</p>

